<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 학생정보 처리 SQL -->
<mapper namespace="student.dao.StudentDAO">

<!-- 학생 정보 저장 -->
<insert id = "" parameterType = "student">
insert into student values(
  id_seq.nextval,
  #{name},
  #{kor},
  #{eng},
  #{mat}
)
</insert>


<!-- 전체 학생 목록 읽기 -->
<select id = "" parameterType = "student">
</select>


<!-- 학번으로 학생정보 삭제 -->
<delete id = "" parameterType = "student">
</delete>


<!-- 학생 정보 수정 -->
<update id = "" parameterType = "student">
</update>


<!-- 이름으로 학생정보 검색 -->
<select id = "" parameterType = "">

</select>


</mapper>
